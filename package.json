{
  "name": "@mmintel/datocms-tools",
  "version": "0.0.16",
  "description": "Useful tools for DatoCMS",
  "main": "dist/lib.min.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/*.js",
    "build": "rollup -c",
    "reset": "node -r esm scripts/reset.js",
    "clear-relations": "node -r esm scripts/clear-relations.js",
    "export-models": "node -r esm scripts/export-models.js",
    "import-models": "node -r esm scripts/import-models.js",
    "extract-models": "node -r esm scripts/extract-models.js",
    "export-menu": "node -r esm scripts/export-menu.js",
    "import-menu": "node -r esm scripts/import-menu.js",
    "release": "npm run test && standard-version && git push --follow-tags && npm publish --access=public"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "commitlint": "^8.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "jest": "^24.1.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.2",
    "standard-version": "^7.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "dependencies": {
    "datocms-client": "^2.0.9",
    "esm": "^3.2.25",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "yargs": "^14.0.0"
  }
}
